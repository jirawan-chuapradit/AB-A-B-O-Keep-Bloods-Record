<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="wrap_content"
        android:layout_height="134dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        app:srcCompat="@drawable/tab" />


    <TextView
        android:id="@+id/notify_item_text"
        android:layout_width="221dp"
        android:layout_height="45dp"
        android:layout_alignStart="@+id/notify_item_date"
        android:layout_alignParentTop="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="25dp"
        android:text="sssssssss"
        android:textColor="@color/whiteColor"
        android:textSize="30sp"
        android:fontFamily="@font/font"
        android:textStyle="bold" />


    <TextView
        android:id="@+id/notify_item_date"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="87dp"
        android:layout_marginEnd="71dp"
        android:fontFamily="@font/font"
        android:textSize="25dp"
        android:text="daaaaaa"
        android:textColor="@color/whiteColor" />


    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="146dp"
        android:layout_height="134dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        app:srcCompat="@drawable/tab_logo" />


</RelativeLayout>